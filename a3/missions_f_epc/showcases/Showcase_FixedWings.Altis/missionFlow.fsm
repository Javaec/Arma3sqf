                /*%FSM<STATE "NEW_Task___Savegame">*/
                class NEW_Task___Savegame
                {
                        name = "NEW_Task___Savegame";
                        itemno = 12;
                        init = /*%FSM<STATEINIT""">*/"// ***************************" \n
                         "// Conditions for Rough landing end" \n
                         "// ***************************" \n
                         "" \n
                         "[] spawn {" \n
                         "" \n
                         "WaitUntil " \n
                         "	{" \n
                         "		(vehicle BIS_Player == BIS_Plane) and" \n
                         "		(BIS_Away) and" \n
                         "		(isTouchingGround BIS_Plane) and" \n
                         "		(damage BIS_Plane < 1) and" \n
                         "		(speed BIS_Plane < 25)" \n
                         "	};" \n
                         "" \n
                         "BIS_RoughLanding = true;" \n
                         "" \n
                         "};" \n
                         "" \n
                         "// ******************" \n
                         "// Weaponry usage FSM - disabled, it's colliding with other conversations" \n
                         "// ******************" \n
                         "" \n
                         "// WeaponryUsage = BIS_missionScope execFSM ""Scripts\WeaponryUsage.fsm"";" \n
                         "" \n
                         "// ************************" \n
                         "// Setting WP on leading vehicle" \n
                         "// ************************" \n
                         "" \n
                         "ConvoyWP = BIS_missionScope execFSM ""Scripts\ConvoyWP.fsm"";" \n
                         "" \n
                         "// *******************" \n
                         "// Navigation Hangar FSM" \n
                         "// *******************" \n
                         "" \n
                         "NavigationHangar = BIS_missionScope execFSM ""Scripts\NavigationHangar.fsm"";" \n
                         "" \n
                         "// ********************" \n
                         "// Navigation Resupply FSM" \n
                         "// ********************" \n
                         "" \n
                         "NavigationResupply = BIS_missionScope execFSM ""Scripts\NavigationResupply.fsm"";" \n
                         "" \n
                         "// ***************" \n
                         "// Conversations FSM" \n
                         "// ***************" \n
                         "" \n
                         "Conversations = BIS_missionScope execFSM ""Scripts\Conversations.fsm"";" \n
                         "" \n
                         "// ***************************************************" \n
                         "// Deleting unnecessary units and objects, moving damaged plane" \n
                         "// back to hangar so player can roll back to his hangar more easily " \n
                         "// ***************************************************" \n
                         "" \n
                         "{deleteVehicle _x} forEach " \n
                         "[" \n
                         "	BIS_Repairman_01," \n
                         "	BIS_Repairman_02, " \n
                         "	BIS_Quad, " \n
                         "	BIS_Hunter, " \n
                         "	BIS_TowerObserver, " \n
                         "	BIS_TowerGuard_01, " \n
                         "	BIS_TowerGuard_02, " \n
                         "	BIS_Launchers_01, " \n
                         "	BIS_Launchers_02, " \n
                         "	BIS_Sign_01, " \n
                         "	BIS_Sign_02," \n
                         "	BIS_plane_damaged," \n
                         "	BIS_plane_damaged_repair" \n
                         "];" \n
                         "" \n
                         "// ********************************" \n
                         "// Destroying empty vehicles near frontline" \n
                         "// to simulate results of  a fierce battle" \n
                         "// ********************************" \n
                         "" \n
                         "{_x setDamage 1} forEach " \n
                         "[" \n
                         "	BIS_Wreck_01, " \n
                         "	BIS_Wreck_02, " \n
                         "	BIS_Wreck_03, " \n
                         "	BIS_Wreck_04, " \n
                         "	BIS_Wreck_05, " \n
                         "	BIS_Wreck_06, " \n
                         "	BIS_Wreck_07, " \n
                         "	BIS_Wreck_08" \n
                         "];" \n
                         "" \n
                         "// *******" \n
                         "// Take off" \n
                         "// *******" \n
                         "" \n
                         "[] spawn {" \n
                         """InTheAir"" call BIS_fnc_missionConversations;" \n
                         "" \n
                         "sleep 5;" \n
                         "saveGame;" \n
                         "" \n
                         "sleep 5;" \n
                         "BIS_TakeOffMusic = true;" \n
                         "" \n
                         "sleep 15;" \n
                         "If ((vehicle BIS_Player == BIS_Plane) and (canMove BIS_Heli)) Then {" \n
                         "HeliHit = BIS_missionScope execFSM ""Scripts\HeliInterception.fsm"";" \n
                         """HeliSpotted"" call BIS_fnc_missionConversations;" \n
                         "BIS_HeliTaskReceived = true;" \n
                         """BIS_mrkHeli"" setMarkerAlpha 1;" \n
                         """BIS_tskHeli"" call BIS_fnc_missionTasks};" \n
                         "" \n
                         "};" \n
                         "" \n
                         "// *********************************" \n
                         "// Friendly fire check for more of allied units" \n
                         "// *********************************" \n
                         "" \n
                         "BIS_Crewman_01 addEventHandler [""Killed"", {(If (_this select 1 == (vehicle BIS_Player))) Then {BIS_BlueOnBlue = true}}];" \n
                         "BIS_Crewman_02 addEventHandler [""Killed"", {(If (_this select 1 == (vehicle BIS_Player))) Then {BIS_BlueOnBlue = true}}];" \n
                         "BIS_Crewman_03 addEventHandler [""Killed"", {(If (_this select 1 == (vehicle BIS_Player))) Then {BIS_BlueOnBlue = true}}];" \n
                         "BIS_Crewman_04  addEventHandler [""Killed"", {(If (_this select 1 == (vehicle BIS_Player))) Then {BIS_BlueOnBlue = true}}];" \n
                         "BIS_Technician_01 addEventHandler [""Killed"", {(If (_this select 1 == (vehicle BIS_Player))) Then {BIS_BlueOnBlue = true}}]; " \n
                         "BIS_Technician_02 addEventHandler [""Killed"", {(If (_this select 1 == (vehicle BIS_Player))) Then {BIS_BlueOnBlue = true}}];" \n
                         "BIS_Technician_03 addEventHandler [""Killed"", {(If (_this select 1 == (vehicle BIS_Player))) Then {BIS_BlueOnBlue = true}}];" \n
                         "" \n
                         "// ****************************************" \n
                         "// Adding score for destroying MLRS supply vehicles" \n
                         "// ****************************************" \n
                         "" \n
                         "BIS_SPG_01_Ammo addEventHandler [""Killed"", {(If (_this select 1 == (vehicle BIS_Player))) Then {BIS_Player addRating 750}}];" \n
                         "BIS_SPG_02_Ammo addEventHandler [""Killed"", {(If (_this select 1 == (vehicle BIS_Player))) Then {BIS_Player addRating 750}}];" \n
                         "" \n
                         "// *******************************************" \n
                         "// Just in case when the plane somehow cannot lock the" \n
                         "//  heli with missiles because it doesn't 'know' about it" \n
                         "// *******************************************" \n
                         "" \n
                         "[] spawn {" \n
                         "WaitUntil {BIS_Plane distance BIS_Heli < 2000};" \n
                         "" \n
                         "(driver BIS_Plane) reveal BIS_Heli;" \n
                         "" \n
                         "};" \n
                         "" \n
                         "// ***************************************" \n
                         "// Create smoke shell when the heli is about to land" \n
                         "// ***************************************" \n
                         "" \n
                         "[] spawn {" \n
                         "" \n
                         "WaitUntil {BIS_Heli distance BIS_LZ < 1250};" \n
                         "" \n
                         "_shellType = ""SmokeShellRed""; " \n
                         "_shell = _shellType createVehicle getMarkerPos ""BIS_mrkLZ"";" \n
                         "" \n
                         "};" \n
                         "" \n
                         "// ****************************************" \n
                         "// Add event handers for being hit to addCamShake" \n
                         "// ****************************************" \n
                         "" \n
                         "[] spawn {" \n
