                /*%FSM<STATE "Move_to_rendezvo">*/
                class Move_to_rendezvo
                {
                        name = "Move_to_rendezvo";
                        itemno = 43;
                        init = /*%FSM<STATEINIT""">*/"// Stop movement order spam" \n
                         "[] spawn {" \n
                         "	scriptName ""missionFlow.fsm: enableSentences control"";" \n
                         "	" \n
                         "	while {isNil ""BIS_atRV""} do {" \n
                         "		BIS_inCombat = nil;" \n
                         "		enableSentences false;" \n
                         "		" \n
                         "		// Wait for the player's team to approach enemies" \n
                         "		waitUntil {" \n
                         "			{_unit = _x; {side _x == RESISTANCE} count (_unit nearEntities [""Man"", 200]) > 0} count units BIS_survivorGrp > 0" \n
                         "			||" \n
                         "			behaviour BIS_exp == ""COMBAT""" \n
                         "			||" \n
                         "			!(isNil ""BIS_atRV"")" \n
                         "		};" \n
                         "		" \n
                         "		if (isNil ""BIS_atRV"") then {" \n
                         "			BIS_inCombat = true;" \n
                         "			enableSentences true;" \n
                         "			" \n
                         "			if (!(isNil ""BIS_stayAlert"") && isNil ""BIS_birds"") then {" \n
                         "				// Make birds fly away" \n
                         "				BIS_birds = true;" \n
                         "				" \n
                         "				private [""_pos""];" \n
                         "				_pos = [BIS_exp, 20, (direction BIS_exp + random 360)] call BIS_fnc_relPos;" \n
                         "				_pos set [2, 10];" \n
                         "				" \n
                         "				BIS_soundLogic setPos _pos;" \n
                         "				BIS_soundLogic say3D ""Scared_Animal6"";" \n
                         "			};" \n
                         "			" \n
                         "			waitUntil {" \n
                         "				(" \n
                         "					{_unit = _x; {side _x == RESISTANCE} count (_unit nearEntities [""Man"", 200]) > 0} count units BIS_survivorGrp == 0" \n
                         "					&&" \n
                         "					behaviour BIS_exp != ""COMBAT""" \n
                         "				)" \n
                         "				||" \n
                         "				!(isNil ""BIS_atRV"")" \n
                         "			};" \n
                         "			" \n
                         "			if (isNil ""BIS_atRV"") then {" \n
                         "				sleep 1;" \n
                         "			};" \n
                         "		};" \n
                         "	};" \n
                         "};" \n
                         "" \n
                         "// Take player off captive if he goes too far" \n
                         "[] spawn {" \n
                         "	scriptName ""missionFlow.fsm: captive control"";" \n
                         "	" \n
                         "	while {isNil ""BIS_stayAlert""} do {" \n
                         "		waitUntil {BIS_inf distance BIS_exp > 50 || !(isNil ""BIS_stayAlert"")};" \n
                         "		" \n
                         "		if (isNil ""BIS_stayAlert"") then {" \n
                         "			BIS_inf setCaptive false;" \n
                         "			" \n
                         "			waitUntil {BIS_inf distance BIS_exp <= 50 || !(isNil ""BIS_stayAlert"")};" \n
                         "			" \n
                         "			if (isNil ""BIS_stayAlert"") then {" \n
                         "				BIS_inf setCaptive true;" \n
                         "			};" \n
                         "		};" \n
                         "	};" \n
                         "};" \n
                         "" \n
                         "// Unassign the player from his vehicle" \n
                         "group BIS_inf leaveVehicle (assignedVehicle BIS_inf);" \n
                         "" \n
                         "// Lock all cars" \n
                         "{_x lock true} forEach (entities ""Car"");" \n
                         "" \n
                         "// Set player to captive" \n
                         "BIS_inf setCaptive true;" \n
                         "" \n
                         "BIS_survivor1 enableAI ""MOVE"";" \n
                         "BIS_survivor1 setBehaviour ""AWARE"";" \n
                         "BIS_survivor1 setUnitPos ""AUTO"";" \n
                         "BIS_survivor1 reveal BIS_exp;" \n
                         "BIS_survivor1 doWatch BIS_exp;" \n
                         "" \n
                         "BIS_survivor2 enableAI ""MOVE"";" \n
                         "BIS_survivor2 setBehaviour ""AWARE"";" \n
                         "BIS_survivor2 setUnitPos ""AUTO"";" \n
                         "BIS_survivor2 reveal BIS_exp;" \n
                         "BIS_survivor2 doWatch BIS_exp;" \n
                         "" \n
                         "BIS_survivor3 enableAI ""MOVE"";" \n
                         "" \n
                         """BIS_survivors"" call BIS_fnc_missionTasks;" \n
                         "" \n
                         "// Remove Instructor Figure" \n
                         "[""RemoveTopic"", [""i01_Navigation""]] call BIS_fnc_InstructorFigure;" \n
                         "" \n
                         "BIS_atSurvivors = [] spawn {" \n
                         "	scriptName ""missionFlow.fsm: move to rendezvous"";" \n
                         "	" \n
                         "	""BIS_regrouped"" call BIS_fnc_missionConversations;" \n
                         "	" \n
                         "	sleep 1;" \n
                         "	" \n
                         "	saveGame;" \n
                         "	" \n
                         "	sleep 1;" \n
                         "	" \n
                         "	// Hide their waypoints" \n
                         "	{_x setWaypointVisible false} forEach waypoints BIS_survivorGrp;" \n
                         "	" \n
                         "	// Join player to leader" \n
                         "	BIS_inf joinAsSilent [BIS_exp, 5];" \n
                         "	" \n
                         "	[] call BIS_joinSurvivors;" \n
                         "	" \n
                         "	// Unhide markers" \n
                         "	{_x setMarkerAlpha 1} forEach [""BIS_RV"", ""BIS_route1"", ""BIS_route2"", ""BIS_route3""];" \n
                         "	" \n
                         "	// Ensure task is shown" \n
                         "	""BIS_RV"" call BIS_fnc_missionTasks;" \n
                         "	" \n
                         "	0 fadeMusic 0.2;" \n
                         "	playMusic ""EventTrack01a_F_EPA"";" \n
                         "	" \n
                         "	// Reset fatigue" \n
                         "	[BIS_inf, 0] call BIS_fnc_camp_setFatigue;" \n
                         "	" \n
                         "	// Move to rendezvous" \n
                         "	BIS_toRV = true;" \n
                         "	" \n
                         "	{_x doWatch objNull} forEach [BIS_exp, BIS_survivor3];" \n
                         "	" \n
